
<ul id = "project_list">
	<div class = "project_box">
  	<div class = "project_title"><%= link_to project.projectname , project_path(project), :id => "project_name_link_#{project.id}" %>

 			<%= render partial:'projects/edit_project',locals: {project: project}%>
 		
 			<div class = "links">
 				<%= link_to "edit",'#',  "data-id" => project.id ,id: "project_Edit_link"  %> | 
 				<%= link_to "delete",project, confirm: "Are you want to delete?", method: :delete %>
 			</div>
 	  </div>
 	
    <span class = "sequence_count">Sequences : <%= project.sequences.count %></span> 
  </div>
  <br>
</ul>
